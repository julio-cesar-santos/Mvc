<IfModule mod_rewrite.c>
    RewriteEngine On
    # Altere a linha abaixo se o seu projeto n√£o estiver na raiz do servidor
    RewriteBase /SiteQueijaria_MVC/public/

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php?url=$1 [L,QSA]
</IfModule>